{
  "name": "tr-juke-box",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "dependencies": {
    "@remix-run/react": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "react-sound": "1.2.0",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "typescript": "^4.1.2",
    "@dvnch/eslint-config": "^2.0.0",
    "@types/node": "^16.11.10",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "prettier": "^2.5.0",
    "vercel": "^23.1.2"
  },
  "scripts": {
    "postinstall": "remix setup node",
    "css": "npx tailwindcss -i ./css/index.css -o ./app/styles/index.css --minify",
    "css:dev": "npx tailwindcss -i ./css/index.css -o ./app/styles/index.css --watch",
    "build": "yarn css && remix build",
    "dev": "concurrently \"yarn css:dev\" \"remix dev\"",
    "start": "vercel dev"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
